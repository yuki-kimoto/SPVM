class TestCase::Precedence {
  static method basic : int () {
    {
      my $i = 0;
      my $ret = (string)$i++;
      unless ($ret eq "0" && $i == 1) {
        return 0;
      }
    }
    
    {
      my $ret = (string)TestCase::Precedence->stab_sum(1, 3);
      unless ($ret eq "4") {
        return 0;
      }
    }
    
    return 1;
  }
  
  static method stab_sum : int ($num1 : int, $num2 : int) {
    return $num1 + $num2;
  }
}
