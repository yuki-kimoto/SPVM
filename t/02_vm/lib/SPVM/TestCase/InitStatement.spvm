class TestCase::InitStatement {
  use TestCase::Empty;
  
  our $FOO : int;
  
  INIT {
    $FOO = 1;
    
    # Call a default INIT block
    Int->INIT;
    TestCase::Empty->INIT;
  }
  
  # Array store undef
  static method init_class_var : int () {
    
    unless ($FOO == 1) {
      return 0;
    }
    
    return 1;
  }
}
