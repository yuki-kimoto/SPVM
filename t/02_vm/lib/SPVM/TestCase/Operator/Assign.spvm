  static method evaluate_left_to_right : int () {
    {
      my $i = 2;
      my $j = ($i = 3) * $i;
      
      unless ($j == 9) {
        return 0;
      }
    }
    
    {
      my $j = 10;
      eval {
        TestCase::Operator::Assign->evaluate_left_to_right_excpetion() / ($j = 3);
      };
      $@ = undef;
      
      unless ($j == 10) {
        return 0;
      }
    }
    
    return 1;
  }
  
