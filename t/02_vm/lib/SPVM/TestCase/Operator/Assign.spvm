  static method assign_cannot_assign : int () {
    
    my $value = (object)new Int[0];
    
    eval { my $value2 = (Long[])$value; }
    
    unless (Fn->contains($@, "An assignment failed. Int[] type cannot be assigned to Long[] type.")) {
      return 0;
    }
    
    return 1;
  }
  
  static method to_mulnum : int () {
    
