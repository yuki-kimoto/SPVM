class TestCase::Module::Sync::Mutex {
  
  use Sync::Mutex;
  use TestCase::Util::Thread;
  
  static method basic : int () {
    
    my $mutex = Sync::Mutex->new;
    
    $mutex->lock;
    
    $mutex->unlock;
    
    $mutex->reader_lock;
    
    $mutex->reader_unlock;
    
    return 1;
  }
}
