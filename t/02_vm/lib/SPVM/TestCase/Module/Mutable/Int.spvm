class TestCase::Module::Mutable::Int {
  use Fn;
  use Array;
  use Mutable::Int;
  
  static method basic : int () {
    my $oval = Mutable::Int->new(Fn->INT32_MIN());
    my $value1 = $oval->value;
    
    unless ($value1 == Fn->INT32_MIN()) {
      return 0;
    }

    $oval->set_value(5);
    
    unless ($oval->value == 5) {
      return 0;
    }
    
    
    return 1;
  }
}
