class TestCase::Module::Mutable::Float {
  use Fn;
  use Array;
  use Mutable::Float;
  
  static method basic : int () {
    my $oval = Mutable::Float->new(Fn->FLT_MIN());
    my $value1 = $oval->value;
    
    unless ($value1 == Fn->FLT_MIN()) {
      return 0;
    }

    $oval->set_value(5f);
    
    unless ($oval->value == 5) {
      return 0;
    }
    
    
    return 1;
  }
}
