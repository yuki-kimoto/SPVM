class TestCase::LineDirective {

  static method line_directive : int () {

#line 1
    my $line = __LINE__;
    
    unless ($line == 1) {
      return 0;
    }
    
#line 73
    my $line2 = __LINE__;
    
    unless ($line2 == 73) {
      return 0;
    }
    
#line 75 "/Foo/Bar.spvm"
    my $line3 = __LINE__;
    my $file3 = __FILE__;
    
    unless ($line3 == 75) {
      return 0;
    }
    
    unless ($file3 eq "/Foo/Bar.spvm") {
      return 0;
    }
    
    return 1;
  }
}
