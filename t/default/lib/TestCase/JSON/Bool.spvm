package TestCase::JSON::Bool {
  use SPVM::Webkit::JSON::Bool;
  sub basic : int () {
    my $true = SPVM::Webkit::JSON::Bool->TRUE;
    my $false = SPVM::Webkit::JSON::Bool->FALSE;
    
    unless ($true->val == 1) {
      return 0;
    }
    
    unless ($false->val == 0) {
      return 0;
    }
    
    unless ($true == SPVM::Webkit::JSON::Bool->TRUE) {
      return 0;
    }
    
    unless ($false == SPVM::Webkit::JSON::Bool->FALSE) {
      return 0;
    }
    
    return 1;
  }
}
