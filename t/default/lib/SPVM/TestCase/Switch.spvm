          case 3: {
            $success = 0;
            break;
          }
          case 4: {
            $success = 0;
            break;
          }
          case 5: {
            $success = 0;
            break;
          }
          default: {
            $success = 1;
          }
        }
        unless ($success) {
          return 0;
        }
      }
      
      # Only default
      {
        my $success : int;
        switch (1) {
          default: {
            $success = 1;
          }
        }
        unless ($success) {
          return 0;
        }
      }

      # No cases and default
      {
        my $success = 0;
        switch (1) {}
        $success = 1;
        unless ($success) {
          return 0;
        }
      }

      # No cases and default
      {
        my $success = 0;
        switch ((byte)1) {
          case 1: {
            $success = 1;
            break;
          }
        }
        unless ($success) {
          return 0;
        }
      }

      # break is added to the end of the block if it doesn't exist
      {
        my $success = 0;
        switch (1) {
          case 1: {
            $success = 1;
          }
          case 2: {
            $success = 2;
          }
        }
        unless ($success == 1) {
          return 0;
        }
      }
      {
        my $success = 0;
        switch (1) {
          case 1: {}
          case 2: {
            $success = 2;
          }
        }
        unless ($success == 0) {
          return 0;
        }
      }

      # break in default
      {
        my $success = 0;
        switch (1) {
          default: {
            $success = 1;
            break;
            $success = 2;
          }
        }
        unless ($success == 1) {
          return 0;
        }
      }
    }
    
    # return statement in the switch statement
    {
      my $ret = TestCase::Switch->switch_return_method();
      
      unless ($ret == 5) {
        return 0;
      }
    }
    
    # No default branch
    {
      my $i = 0;
      switch ($i) {
        case 1: {
          1;
