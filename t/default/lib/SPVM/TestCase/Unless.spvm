package SPVM::TestCase::Unless {
  sub unless_ok : int () {
    unless(0) {
      return 1;
    }
    
    return 0;
  }

  sub unless_not_ok : int ()  {
    unless(1) {
      return 0;
    }
    
    return 1;
  }
  
  sub unless_else : int ()  {
    unless (1) {
      return 0;
    }
    else {
      return 1;
    }
    
    return 0;
  }
}
