    return 1;
  }
  
  static method match_char_class : int () {

    # Match char cap1
    {
      {
        my $re = Regex->new("[bcdあ]+");
        my $target = "zabcあz";
        
        my $match = $re->match($target, 0);
        
        unless ($match) {
          return 0;
        }
      }
      
      {
        my $re = Regex->new("^[a-c\dx-z]+$", "a");
        my $target = "ac09xz";
        my $match = $re->match($target, 0);
        unless ($match) {
          return 0;
        }
      }
    }

    # Match char class \d
    {
      my $re = Regex->new_with_options("[\da]+", "a");
      my $target = "z01az";
      
      my $match = $re->match($target, 0);
      
      unless ($match) {
        return 0;
      }
    }

    # Not Match char cap1
    {
      my $re = Regex->new_with_options("[bcd\d]+", "a");
      my $target = "ae";
      
      my $match = $re->match($target, 0);
      
      if ($match) {
        return 0;
      }
    }

    # ^[\d\D]+
    {
      my $re = Regex->new_with_options("^[\d\D]+$", "a");
      
      my $target = "abc123";
      
      my $match = $re->match($target, 0);
      
      unless ($match) {
        return 0;
      }
    }

    # ^[^\d\D]+
    {
      my $re = Regex->new_with_options("^[^\d\D]+$", "a");
      
      {
        my $target = "abc";
        
        my $match = $re->match($target, 0);
        if ($match) {
          return 0;
        }
      }
      {
        my $target = "123";
        
        my $match = $re->match($target, 0);
        if ($match) {
          return 0;
        }
      }
    }

    # [a-a] | "-" is a character unless "-" is used as a charcter cap1 range
    {
      my $re = Regex->new("[a-a]");
      
      {
        my $target = "a";
        my $match = $re->match($target, 0);
        unless ($match) {
          return 0;
        }
      }
    }
    
    # [a-aa-a] | "-" is a character unless "-" is used as a charcter cap1 range
    {
      my $re = Regex->new("[a-aa-a]");
      
      {
        my $target = "a";
        my $match = $re->match($target, 0);
        unless ($match) {
          return 0;
        }
      }
    }

    # - | "-" is a character unless "-" is used as a charcter cap1 range
    {
      {
        my $re = Regex->new("-");
        
        {
          my $target = "-";
          my $match = $re->match($target, 0);
          unless ($match) {
            return 0;
          }
        }
      }
      {
        my $re = Regex->new("a-z");
        
        {
          my $target = "a-z";
          my $match = $re->match($target, 0);
          unless ($match) {
            return 0;
          }
        }
      }
      
    }

    # [-] | "-" is a character unless "-" is used as a charcter cap1 range
    {
      my $re = Regex->new("[-]");
      
      {
        my $target = "-";
        my $match = $re->match($target, 0);
        unless ($match) {
          return 0;
        }
      }
    }

    # [a-] | "-" is a character unless "-" is used as a charcter cap1 range
    {
      my $re = Regex->new("[a-]");
      
      {
        my $target = "a";
        my $match = $re->match($target, 0);
        unless ($match) {
          return 0;
        }
      }

      {
        my $target = "-";
        my $match = $re->match($target, 0);
        unless ($match) {
          return 0;
        }
      }
    }

    # Invalid character cap1 range
    {
      eval { Regex->new("[z-a]"); };
      
      unless ($@) {
        return 0;
      }
    }

    # [ ^a]
    {
      my $re = Regex->new("[ ^a]");
      
      {
        my $target = " ";
        my $match = $re->match($target, 0);
        unless ($match) {
          return 0;
        }
      }
      {
        my $target = "^";
        my $match = $re->match($target, 0);
        unless ($match) {
          return 0;
        }
      }
      {
        my $target = "a";
        my $match = $re->match($target, 0);
        unless ($match) {
          return 0;
        }
      }
    }
    
    $@ = undef;
    
    return 1;
  }

