      my $is_equals = Fn->equals_array_byte($nums1, $nums4);
      if ($is_equals) {
        return 0;
      }
    }
    
    {
      my $is_equals = Fn->equals_array_byte(undef, undef);
      if ($is_equals) {
        return 1;
