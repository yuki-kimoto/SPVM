  sub test_isinff: int () {
    if (Math->isinff(Math->INFINITYF())) {
      if (!Math->isinff(Math->NANF())) {
        if (!Math->isinff(0.1f)) {
          return 1;
        }
      }
    }

    return 0;
  }

