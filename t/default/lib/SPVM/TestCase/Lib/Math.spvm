  sub test_nearbyint : int () {
    {
      if (Math->fesetround(Math->FE_DOWNWARD)) {
        die "failed to set fesetround(FE_DOWNWARD)";
      }
      {
        my $got = Math->nearbyint(PI());
        my $expected = 3;
        unless ($got == $expected) {
          return 0;
        }
      }
      {
        my $got = Math->nearbyint(-PI());
        my $expected = -4;
        unless ($got == $expected) {
          return 0;
        }
      }
    }
    {
      if (Math->fesetround(Math->FE_TONEAREST)) {
        die "failed to set fesetround(FE_TONEAREST)";
      }
      {
        my $got = Math->nearbyint(PI());
        my $expected = 3;
        unless ($got == $expected) {
          return 0;
        }
      }
      {
        my $got = Math->nearbyint(PI() * 4);
        my $expected = 13;
        unless ($got == $expected) {
          return 0;
        }
      }
    }
    {
      if (Math->fesetround(Math->FE_TOWARDZERO)) {
        die "failed to set fesetround(FE_TOWARDZERO)";
      }
      {
        my $got = Math->nearbyint(PI());
        my $expected = 3;
        unless ($got == $expected) {
          return 0;
        }
      }
      {
        my $got = Math->nearbyint(-PI());
        my $expected = -3;
        unless ($got == $expected) {
          return 0;
        }
      }
    }
    {
      if (Math->fesetround(Math->FE_UPWARD)) {
        die "failed to set fesetround(FE_UPWARD)";
      }
      {
        my $got = Math->nearbyint(PI());
        my $expected = 4;
        unless ($got == $expected) {
          return 0;
        }
      }
      {
        my $got = Math->nearbyint(-PI());
        my $expected = -3;
        unless ($got == $expected) {
          return 0;
        }
      }
    }
    return 1;
  }

