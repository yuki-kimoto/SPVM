      }
    }

    # Negative value
    {
      my $num      = -2147483647;
      my $abs      = Math->abs($num);
      unless ($abs == 2147483647) {
        return 0;
      }
    }
    # 0
    {
      my $num      = 0;
      my $abs      = Math->abs($num);
      unless ($abs == 0) {
        return 0;
      }
    }
    
    return 1;
  }
