class TestCase::Pointer : pointer_t {
  native sub new : TestCase::Pointer ($x : int);
  
  native sub get_x : int ($self : self);
  
  native sub DESTROY : void ($self : self);
  
  sub struct_test : int () {
    my $struct = TestCase::Pointer->new(3);
    my $x = $struct->get_x;
    
    if ($x == 3) {
      return 1;
    }
    
    return 0;
  }
}
