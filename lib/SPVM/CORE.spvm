  precompile sub rindex : int ($text : string, $search : string, $from : int) {
    my $text_len = length $text;
    my $search_len = length $search;
    if ($from > $text_len - $search_len) {
      $from = $text_len - $search_len;
    }
    for (my $i = $from; $i >= 0; --$i) {
      my $match = 1;
      for (my $j = 0; $j < $search_len; ++$j) {
        if ($text->[$i + $j] != $search->[$j]) {
          $match = 0;
          last;
        }
      }
      if ($match) {
        return $i;
      }
    }
    return -1;
  }

  precompile sub starts_with : int ($source : string, $prefix : string) {
    my $source_len = length $source;
    my $prefix_len = length $prefix;
    unless ($source_len >= $prefix_len) {
      return 0;
    }
    for (my $i = 0; $i < $prefix_len; ++$i) {
      unless ($prefix->[$i] == $source->[$i]) {
        return 0;
      }
    }
    return 1;
  }

  precompile sub contains : int ($text : string, $pattern : string) {
    if (!$text) {
      die "Target string must be defined";
    }
    if (!$pattern) {
      die "Search string must be defined";
    }

    my $pat_len = length $pattern;
    my $text_len = length $text;
    my $i = 0;
    my $j = 0;
    
    while ($i < $text_len && $j < $pat_len) {
      if ($text->[$i] == $pattern->[$j]) {
        $i++;
        $j++;
      } else {
        $i = $i - $j + 1;
        $j = 0;
      }
    }
    
    if ($j == $pat_len) {
      return 1;
    }
    else {
      return 0;
    }
  }
  
